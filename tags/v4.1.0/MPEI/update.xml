      <GeneralInfo>
        <Name>MP-TVSeries</Name>
        <Id>5e2777c3-966c-407f-b5a9-e51205b70b3e</Id>
        <Author>ltfearme</Author>
        <HomePage>http://code.google.com/p/mptvseries/</HomePage>
        <ForumPage>http://forum.team-mediaportal.com/my-tvseries-162</ForumPage>
