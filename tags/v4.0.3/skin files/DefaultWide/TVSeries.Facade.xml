<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<window>
  <controls>
    <control>
      <description>group element</description>
      <type>group</type>
      <visible>!Control.HasFocus(50)</visible>
      <animation effect="fade" start="0" end="100" time="200">WindowOpen</animation>
      <animation effect="fade" start="100" end="0" time="200">WindowClose</animation>
      <control>
        <description>composite control consisting of a list control and a thumbnail panel</description>
        <type>facadeview</type>
        <id>50</id>
				<control>
          <description>album view listcontrol</description>
          <type>listcontrol</type>
          <subtype>album</subtype>
          <id>50</id>
        </control>
        <control>
          <description>listcontrol</description>
          <type>listcontrol</type>
          <id>50</id>
          <width>550</width>          
          <textXOff2>550</textXOff2>
          <textYOff2>2</textYOff2>
          <textXOff3>2545</textXOff3>
          <textYOff3>8</textYOff3>
          <textalign3>right</textalign3>
          <font3>font9</font3>
          <scrollOffset>2</scrollOffset>
          <spinCanFocus>no</spinCanFocus>
					<itemWidth>20</itemWidth>
          <itemHeight>20</itemHeight>
					<IconXOff>500</IconXOff>
          <IconYOff>6</IconYOff>
          <textXOff2>498</textXOff2>
					<onright>2</onright>
        </control>
        <control>
          <description>Thumbnail Panel</description>
          <type>thumbnailpanel</type>
          <id>50</id>
          <onleft>2</onleft>
          <onright>2</onright>
          <posX>50</posX>
          <posY>170</posY>
          <width>670</width>
          <height>450</height>
          <textXOff>0</textXOff>
					<textYOff>0</textYOff>
					<frame>-</frame>
					<frameFocus>tvseries_listbanner_focus.png</frameFocus>
					<frameNoFocus>-</frameNoFocus>
					<textureMask>tvseries_listbanner_mask.png</textureMask>
					<keepaspectratio>no</keepaspectratio>
					<enableFocusZoom>no</enableFocusZoom>
					<zoomXPixels>40</zoomXPixels>
          <zoomYPixels>10</zoomYPixels>
          <scrollOffset>1</scrollOffset>
          <dimColor>30ffffff</dimColor>
          <itemWidthBig>330</itemWidthBig>
          <itemHeightBig>74</itemHeightBig>
          <textureWidthBig>305</textureWidthBig>
          <textureHeightBig>68</textureHeightBig>
          <thumbWidthBig>305</thumbWidthBig>
          <thumbHeightBig>68</thumbHeightBig>
          <thumbPosXBig>0</thumbPosXBig>
          <thumbPosYBig>0</thumbPosYBig>
          <thumbZoom>no</thumbZoom>
          <textXOff>0</textXOff>
          <textYOff>0</textYOff>
					<onright>2</onright>
					<spinCanFocus>no</spinCanFocus>
        </control>
        <control>
          <description>Filmstrip view</description>
          <type>filmstrip</type>
          <id>50</id>
          <posX>80</posX>
          <posY>240</posY>
          <width>1180</width>
          <height>500</height>
          <spinPosX>-800</spinPosX>
          <spinPosY>555</spinPosY>
          <onleft>2</onleft>
          <onright>2</onright>
          <onup>2</onup>
          <ondown>2</ondown>
          <frame>videothumbframe.png</frame>
          <frameFocus>videothumbframe.png</frameFocus>
          <itemWidth>160</itemWidth>
          <itemHeight>400</itemHeight>
          <textureWidth>167</textureWidth>
          <textureHeight>241</textureHeight>
          <thumbWidth>167</thumbWidth>
          <thumbHeight>241</thumbHeight>
          <thumbPosX>0</thumbPosX>
          <thumbPosY>0</thumbPosY>
          <showFrame>yes</showFrame>
          <showFolder>no</showFolder>
          <showBackGround>no</showBackGround>
          <showInfoImage>no</showInfoImage>
          <enableFocusZoom>no</enableFocusZoom>
          <scrollOffset>5</scrollOffset>
          <keepaspectratio>no</keepaspectratio>
          <suffix>|</suffix>
          <folderPrefix />
          <folderSuffix />
          <spinCanFocus>no</spinCanFocus>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>320</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <thumbs mask="Videothumbmask.png"></thumbs>
          <thumbAnimation effect="zoom" start="100,100" end="150,150" time="200" tween="circle" ease="inout">focus</thumbAnimation>
          <thumbAnimation effect="zoom" start="150,150" end="100,100" time="200" tween="back" ease="inout">unfocus</thumbAnimation>
        </control>
        <control>
          <description>Cover Flow view</description>
          <type>coverflow</type>
          <colordiffuse>90ffffff</colordiffuse>
          <dimColor>90ffffff</dimColor>
          <id>50</id>
          <posX>0</posX>
          <posY>200</posY>
          <width>1280</width>
          <height>720</height>
          <onup>2</onup>
          <onleft>2</onleft>
          <selectedCard>0</selectedCard>
          <cardWidth>250</cardWidth>
          <cardHeight>360</cardHeight>
          <angle>65</angle>
          <sideShift>300</sideShift>
          <sideGap>100</sideGap>
          <sideDepth>550</sideDepth>
          <offsetY>0</offsetY>
          <selectedOffsetY>0</selectedOffsetY>
          <speed>8</speed>
          <backgroundColor>000000FF</backgroundColor>
          <backgroundHeight>100</backgroundHeight>
          <backgroundWidth>1366</backgroundWidth>
          <backgroundX>0</backgroundX>
          <backgroundY>400</backgroundY>
          <backgroundDiffuse>60000000</backgroundDiffuse>
          <background>roof.png</background>
          <showBackground>yes</showBackground>
          <foregroundHeight>720</foregroundHeight>
          <foregroundWidth>1280</foregroundWidth>
          <foregroundX>0</foregroundX>
          <foregroundY>0</foregroundY>
          <foregroundDiffuse>FF000000</foregroundDiffuse>
          <foreground>-</foreground>
          <showForeground>no</showForeground>
          <showFrame>yes</showFrame>
          <frame>videothumbframe.png</frame>
          <frameFocus>videothumbframe.png</frameFocus>
          <frameWidth>250</frameWidth>
          <frameHeight>360</frameHeight>
          <spinSpeed>8</spinSpeed>
          <unfocusedAlpha>FF</unfocusedAlpha>
          <folderPrefix>[</folderPrefix>
          <folderSuffix>]</folderSuffix>
          <font1>font13</font1>
          <font2>font10</font2>
          <label1>-</label1>
          <label2>-</label2>
          <textColor>FFFFFFFF</textColor>
          <remoteColor>ffcc9900</remoteColor>
          <playedColor>ff6FAFE1</playedColor>
          <downloadColor>ff6FAFE1</downloadColor>
          <selectedColor>ff6FAFE1</selectedColor>
          <shadowAngle>45</shadowAngle>
          <shadowDistance>0</shadowDistance>
          <shadowColor>FF000000</shadowColor>
          <label1YOff>405</label1YOff>
          <label2YOff>430</label2YOff>
          <pageSize>5</pageSize>
          <keepaspectratio>no</keepaspectratio>
          <scrollbarBackground>bar_hor.png</scrollbarBackground>
          <scrollbarLeft>mouse_left_horizontal.png</scrollbarLeft>
          <scrollbarRight>mouse_right_horizontal.png</scrollbarRight>
          <scrollbarYOff>385</scrollbarYOff>
          <showScrollbar>yes</showScrollbar>
          <cards mask="videothumbmask.png">videothumbframe.png</cards>
          <folderPrefix />
          <folderSuffix />
          <subitems>
            <subitem>
              <![CDATA[<controls>
              <control>
                <description>background</description>
                <type>image</type>
                <posX>0</posX>
                <posY>0</posY>
                <width>250</width>
                <height>360</height>
                <texture mask="videothumbmask.png">blue.png</texture>
              </control>
              <control>
                <description>Show Title</description>
                <type>fadelabel</type>
                <posX>10</posX>
                <posY>10</posY>
                <width>230</width>
                <label>#TVSeries.Title</label>
                <font>font11</font>
                <textcolor>ivory</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
              <control>
                <type>imagelist</type>
                <id>1</id>
                <posX>10</posX>
                <posY>40</posY>
                <width>230</width>
                <height>37</height>
                <textureWidth>17</textureWidth>
                <textureHeight>17</textureHeight>
                <subitems>
                  <subitem>greystar.png</subitem>
                  <subitem>star.png</subitem>
                </subitems>
                <orientation>horizontal</orientation>
                <align>left</align>
                <imagesToDraw>10</imagesToDraw>
                <percentage>#TVSeries.Series.Rating</percentage>
              </control>
              <control>
                <description>genre</description>
                <type>fadelabel</type>
                <posX>10</posX>
                <posY>60</posY>
                <width>230</width>
                <label>#(iif(eq(#TVSeries.Series.Genre,''),'N/A','#TVSeries.Series.Genre'))</label>
                <font>font9</font>
                <textcolor>ivory</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
              <control>
                <description>FirstAired</description>
                <type>fadelabel</type>
                <posX>10</posX>
                <posY>80</posY>
                <width>230</width>
                <label>#TVSeries.Series.DataPanelLine2Value</label>
                <font>font9</font>
                <textcolor>ivory</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
              <control>
                <description>Airs</description>
                <type>fadelabel</type>
                <posX>10</posX>
                <posY>100</posY>
                <width>230</width>
                <label>#(iif(eq(#TVSeries.Series.AirsDay,''),'N/A','#TVSeries.Series.AirsDay')) #(iif(eq(#TVSeries.Series.AirsTime,''), '', '@ #TVSeries.Series.AirsTime'))</label>
                <font>font9</font>
                <textcolor>ivory</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
              <control>
                <description>mpaarating</description>
                <type>fadelabel</type>
                <posX>10</posX>
                <posY>120</posY>
                <width>230</width>
                <label>#(iif(eq(#TVSeries.Series.ContentRating,''),'N/A','#TVSeries.Series.ContentRating'))</label>
                <font>font9</font>
                <textcolor>ivory</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
              <control>
                <description>plot</description>
                <type>textboxscrollup</type>
                <posX>10</posX>
                <posY>145</posY>
                <width>240</width>
                <height>82</height>
                <label>#TVSeries.Description</label>
                <font>font9</font>
                <lineSpacing>2.5</lineSpacing>
                <textcolor>ivory</textcolor>
                <shadowAngle>0</shadowAngle>
                <shadowDistance>0</shadowDistance>
              </control>
            </controls>]]></subitem>
        </subitems>
      </control>
    </control>
  </control>
</controls>
</window>
